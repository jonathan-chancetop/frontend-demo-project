{
  "name": "website-frontend",
  "version": "1.0.0",
  "description": "website frontend",
  "scripts": {
    "build": "node webpack/build.js",
    "start": "node webpack/start.js"
  },
  "author": "neo",
  "license": "Apache-2.0",
  "config": {
    "lib": {
      "react": ["react-dom", "react-redux", "redux", "react-router-dom", "redux-saga", "react-router-redux"],
      "net": ["axios"],
      "polyfill": ["@babel/polyfill"]
    },
    "pages": {
      "index": {
        "js": "index.tsx",
        "template": "index.html"
      },
      "error": {
        "js": "error.tsx",
        "template": "error.html"
      }
    }
  },
  "devDependencies": {
    "@types/node": "9.4.7",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-redux": "5.0.15",
    "@types/react-router-dom": "4.2.5",
    "@types/react-router-redux": "5.0.12",
    "autodll-webpack-plugin": "0.3.9",
    "autoprefixer": "8.1.0",
    "chalk": "2.3.2",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.1",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "html-webpack-plugin": "3.0.6",
    "jest": "22.4.2",
    "less": "3.0.1",
    "less-loader": "4.1.0",
    "postcss-loader": "2.1.1",
    "style-loader": "0.20.3",
    "stylelint": "9.1.3",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.3",
    "ts-import-plugin": "1.4.4",
    "ts-loader": "3.5.0",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "url-loader": "1.0.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-parallel-uglify-plugin": "1.1.0",
    "yargs": "11.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.42",
    "antd": "3.3.0",
    "axios": "0.18.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-saga": "0.16.0"
