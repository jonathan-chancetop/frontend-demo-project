{
  "name": "website-frontend",
  "version": "1.0.0",
  "description": "website frontend",
  "author": "neo",
  "license": "Apache-2.0",
  "scripts": {
    "build": "node webpack/build.js",
    "start": "node webpack/start.js",
    "test": "jest --config webpack/jest.json",
    "format": "prettier --config webpack/prettier.json --write \"{src,test}/**/*.{ts,tsx,less}\""
  },
  "config": {
    "lib": {
      "react": [
        "react-dom",
        "react-redux",
        "redux",
        "react-router-dom",
        "redux-saga",
        "connected-react-router"
      ],
      "net": [
        "axios"
      ],
      "polyfill": [
        "@babel/polyfill"
      ]
    },
    "pages": {
      "index": {
        "js": "index.tsx",
        "template": "index.html"
      },
      "error": {
        "js": "error.tsx",
        "template": "error.html"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "@types/node": "10.3.0",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "6.0.0",
    "@types/react-router-dom": "4.2.7",
    "autodll-webpack-plugin": "0.3.9",
    "autoprefixer": "8.5.2",
    "chalk": "2.4.1",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.1",
    "fs-extra": "6.0.1",
    "glob": "7.1.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.1.0",
    "less": "3.0.4",
    "less-loader": "4.1.0",
    "postcss-loader": "2.1.5",
    "prettier": "1.13.4",
    "style-loader": "0.21.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.5",
    "ts-import-plugin": "1.5.0",
    "ts-jest": "22.4.6",
    "ts-loader": "3.5.0",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.8.4",
    "url-loader": "1.0.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-parallel-uglify-plugin": "1.1.0",
    "yargs": "11.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.49",
    "antd": "3.5.4",
    "axios": "0.18.0",
    "connected-react-router": "4.3.0",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0",
    "redux-saga": "0.16.0"
  }
}
