    "react-native-linear-gradient": "2.5.4",
    "react-native-snap-carousel": "3.7.5",
    "react-native-splash-screen": "3.2.0",
    "react-native-webview": "5.8.1",
    "react-navigation": "3.9.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
