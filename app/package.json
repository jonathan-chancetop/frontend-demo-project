{
  "name": "demo",
  "version": "1.0.0",
  "scripts": {
    "start": "react-native start --reset-cache",
    "ios": "react-native run-ios --simulator \"iPhone X\"",
    "android": "react-native run-android",
    "format": "prettier --config node/prettier.json --write \"{app,test}/**/*.{ts,tsx}\"",
    "check": "node node/check.js",
    "debug": "open \"rndebugger://set-debugger-loc?host=localhost&port=8081\"",
    "api": "node node/api.js",
    "module": "node node/module.js",
    "bundle-ios": "react-native bundle --platform ios --entry-file index.js --dev false --reset-cache --bundle-output ./ios/main.jsbundle --assets-dest ios"
  },
  "dependencies": {
    "core-native": "file:../../core-native-project/build/dist",
    "native-base": "^2.12.1",
    "react": "16.8.6",
    "react-native": "0.59.8",
    "react-native-device-info": "2.0.2",
    "react-native-gesture-handler": "1.2.1",
    "react-navigation": "3.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/runtime": "7.4.5",
    "axios": "0.18.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "2.4.2",
    "fs-extra": "8.0.1",
    "metro-react-native-babel-preset": "0.54.1",
    "prettier": "1.17.1",
    "tslint": "5.16.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.5",
    "yargs": "13.2.4"
  }
}
